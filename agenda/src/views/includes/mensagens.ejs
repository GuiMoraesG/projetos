<% if (erros.length > 0) { %>
    <div class="row">

        <div class="col my-3">

            <div class="alert alert-danger">

                <% erros.forEach(erro=> { %>
                    <%= erro %><br>
                <% }) %>

            </div>

        </div>

    </div>
<% }%>

<% if (sucesso.length > 0) { %>
<div class="row">
    
    <div class="col my-3">
    
        <div class="alert alert-success">
            <% sucesso.forEach(erro => { %>
                <%= sucesso %><br>
            <% }) %>
        </div>
        
    </div>

</div>
<% } %>